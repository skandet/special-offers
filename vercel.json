{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/",
      "dest": "app.py"
    },
    {
      "src": "/admin",
      "dest": "app.py"
    },
    {
      "src": "/index.html",
      "dest": "app.py"
    },
    {
      "src": "/admin.html",
      "dest": "app.py"
    }
  ]
}